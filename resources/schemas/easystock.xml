<tables xmlns="http://labkey.org/data/xml">
    <table tableName="Location" tableDbType="TABLE">
        <columns>
            <colum columnName ="RowId">
                <columnTitle>Row Id</columnTitle>
                <isHidden>true</isHidden>
            </colum>
            <colum columnName ="Name"/>
            <colum columnName ="Code">
                <isHidden>true</isHidden>
            </colum>
            <colum columnName ="FullCode">
                <columnTitle>Full Code</columnTitle>
            </colum>
            <colum columnName ="IsPlant"/>
            <column columnName="CountryCode"/>
            <column columnName="CountryName"/>
            <column columnName="City"/>
            <column columnName="State"/>
            <colum columnName ="YearCount"/>

            <colum columnName ="Container"/>
            <colum columnName ="Created"/>
            <colum columnName ="CreatedBy"/>
            <colum columnName ="Modified"/>
            <colum columnName ="ModifiedBy"/>
        </columns>
    </table>

    <table tableName="Product" tableDbType="TABLE">
        <columns>
            <colum columnName ="RowId">
                <columnTitle>Row Id</columnTitle>
            </colum>
            <colum columnName ="Name"/>
            <colum columnName ="CommonName"/>
            <colum columnName ="FullCode">
                <columnTitle>Full Code</columnTitle>
            </colum>
            <colum columnName ="IsPlant"/>
            <column columnName="InternalCode"/>
            <column columnName="Category"/>
            <column columnName="ProductionLine"/>
            <colum columnName ="Description"/>
            <colum columnName ="CurrentStatus"/>

            <colum columnName ="Container"/>
            <colum columnName ="Created"/>
            <colum columnName ="CreatedBy"/>
            <colum columnName ="Modified"/>
            <colum columnName ="ModifiedBy"/>
        </columns>
    </table>

    <table tableName="Production" tableDbType="TABLE">
        <columns>
            <column columnName="RowId">
                <isHidden>true</isHidden>
            </column>
            <column columnName="LocationId">
                <fk>
                    <fkTable>Location</fkTable>
                    <fkColumnName>RowId</fkColumnName>
                </fk>
            </column>
            <column columnName="LotId">
                <fk>
                    <fkTable>ProductionLot</fkTable>
                    <fkColumnName>RowId</fkColumnName>
                </fk>
            </column>
            <column columnName="ProductionDate"/>
            <column columnName="ExpirationDate"/>
            <column columnName="BatchName"/>
            <column columnName="ProductionBatch"/>
            <column columnName="ProductName"/>
            <column columnName="InternalCode"/>
            <column columnName="Operator"/>
            <column columnName="Humidity">
                <formatString>0.0%</formatString>
            </column>
            <column columnName="NetWeight"/>
            <column columnName="UoW">
                <columnTitle>Weight Unit</columnTitle>
            </column>
            <column columnName="Quantity"/>
            <column columnName="UoM">
                <columnTitle>Quantity Unit</columnTitle>
            </column>
            <column columnName="PackageAmount"/>
            <column columnName="UoP">
                <columnTitle>Package Type</columnTitle>
            </column>
            <colum columnName ="Container"/>
            <colum columnName ="Created"/>
            <colum columnName ="CreatedBy"/>
            <colum columnName ="Modified"/>
            <colum columnName ="ModifiedBy"/>
        </columns>
    </table>

    <table tableName="ProductionLot" tableDbType="TABLE">
        <columns>
            <column columnName="RowId"/>
            <column columnName="Lot"/>
            <column columnName="Reference"/>
            <colum columnName ="Container"/>
            <colum columnName ="Created"/>
            <colum columnName ="CreatedBy"/>
            <colum columnName ="Modified"/>
            <colum columnName ="ModifiedBy"/>
        </columns>
    </table>

    <table tableName="SampleInventory" tableDbType="TABLE">
        <columns>
            <column columnName="RowId">
                <isHidden>true</isHidden>
            </column>
            <column columnName="LocationId">
                <fk>
                    <fkTable>Location</fkTable>
                    <fkColumnName>RowId</fkColumnName>
                </fk>
            </column>
            <column columnName="LotId">
                <columnTitle>Lot#</columnTitle>
                <fk>
                    <fkTable>ProductionLot</fkTable>
                    <fkColumnName>RowId</fkColumnName>
                </fk>
            </column>
            <column columnName="Quantity"/>
            <column columnName="UoM"/>
            <column columnName="Room"/>
            <column columnName="Shelf"/>
            <column columnName="Box"/>
            <colum columnName ="Container"/>
            <colum columnName ="Created"/>
            <colum columnName ="CreatedBy"/>
            <colum columnName ="Modified"/>
            <colum columnName ="ModifiedBy"/>
        </columns>
    </table>
</tables>
